{
  "problemMatcher": [
    {
      "owner": "deno-test",
      "fileLocation": ["relative", "${cwd}"],
      "pattern": [
        {
          "regexp": "^.+ => ([\\\\/\\w-_\\.]+):(\\d):(\\d)$",
          "file": 1,
          "line": 2,
          "column": 3
        },
        {
          "regexp": "^(error|warning): (.*)$",
          "severity": 1,
          "message": 2,
          "loop": true
        }
      ]
    },
    {
      "owner": "deno-lint-compact",
      "pattern": [
        {
          "regexp": "^((\\w:)?[\\\\/\\w-_\\.]+): line (\\d+), col (\\d+) - (.+) (\\(.+\\))",
          "file": 1,
          "line": 3,
          "column": 4,
          "message": 5,
          "code": 6
        }
      ]
    }
  ]
}
